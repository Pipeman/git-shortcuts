alias gitgraph='git log --graph --all --oneline --decorate'
alias fetch='git fetch'
alias funpm='rm -rf node_modules/ package-lock.json'
alias fulock='rm package-lock.json'
alias gst='git status'
alias gc='git commit'
alias gca='git commit -a'
alias gcaa='git commit -a --amend'
alias gp='git push'
alias gpl='git push --force-with-lease'
alias gg='gitgraph'
alias gf='fetch'
alias gri='git rebase -i'
alias gaa='git add -A'
alias grh='git reset --hard'
alias grhom='grh origin/master'
